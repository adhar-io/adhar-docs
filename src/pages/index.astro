    subscriptionData,
    howWorksData,
    AdharSolutionData,
    KubernetesDeploymentData,
    featuresData,
    ourteamData,
    WorkWithUsData,
} from "@/data";
import Subscription from "@/components/Subscription.astro";
import HowWorks from "@/components/how-works.astro";
